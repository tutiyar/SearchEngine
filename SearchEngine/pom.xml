    <artifactId>tika-app</artifactId>
    <version>1.16</version>
</dependency>
    
    
  </dependencies>
  
  <build>
    <finalName>SearchEngine</finalName>
    <plugins>
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
